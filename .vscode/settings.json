{
    "typescript.tsdk": "./node_modules/typescript/lib",
    "editor.rulers": [80],
    "editor.tabSize": 2,
    "editor.wrappingIndent": "indent",
    "editor.renderWhitespace": "boundary",
    "search.exclude": {
      "**/node_modules": true,
      "**/build": true
    },
    "files.exclude": {
      "**/.git": true,
      "**/.idea": true,
      "**/.vscode": true,
      "**/build": true,
      "**/node_modules": true
    },
    "vsicons.presets.angular": false,
    "TodoParser": {
      "folderExclude": ["node_modules", ".vscode"]
    }
  }